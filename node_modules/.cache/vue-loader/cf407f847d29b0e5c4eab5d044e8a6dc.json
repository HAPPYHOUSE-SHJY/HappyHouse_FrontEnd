  <table>\r\n          <tr>\r\n            <th>\r\n              <label for=\"title\">제목</label>\r\n            </th>\r\n            <td>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                id=\"title\"\r\n                ref=\"title\"\r\n                placeholder=\"제목을 입력하세요\"\r\n                v-model=\"title\"\r\n              />\r\n            </td>\r\n          </tr>\r\n\r\n          <tr>\r\n            <th><label for=\"content\">내용</label></th>\r\n            <td>\r\n              <textarea\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                id=\"contnet\"\r\n                ref=\"content\"\r\n                placeholder=\"내용을 입력하세요\"\r\n                v-model=\"content\"\r\n              ></textarea>\r\n            </td>\r\n          </tr>\r\n        </table>\r\n      </div>\r\n      <br /><br />\r\n      <v-btn class=\"completeBtn\" color=\"blue\" style=\"width: 80px; left: 45%\" dark @click=\"check\">\r\n        등록\r\n      </v-btn>\r\n    </div>\r\n\r\n    <div v-else style=\"margin: 0 auto\">\r\n      <h4>성공적으로 글등록이 완료하였습니다!</h4>\r\n      <router-link class=\"btn btn-primary\" to=\"/insertboard\">새 글 작성</router-link>\r\n      |\r\n      <router-link class=\"btn btn-primary\" to=\"/qnaboard\">목록으로</router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nimport http from \"@/util/http-common\";\r\n//import axios from 'axios';\r\n//const SERVER_URL = process.env.VUE_APP_SERVER_URL;\r\nexport default {\r\n  name: \"InsertBoard\",\r\n  data: function () {\r\n    return {\r\n      info: null,\r\n      loading: true,\r\n      errored: false,\r\n      writer: \"\",\r\n      content: \"\",\r\n      no: \"\",\r\n      regtime: \"\",\r\n      title: \"\",\r\n      submitted: false,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getAccessToken\", \"getUserId\", \"getUserName\", \"getIsAdmin\"]),\r\n  },\r\n  methods: {\r\n    check() {\r\n      let err = true;\r\n      let msg = \"\";\r\n      // !this.writer && ((msg = \"작성자를 입력해주세요\"), (err = false), this.$refs.writer.focus());\r\n      err && !this.title && ((msg = \"제목 입력해주세요\"), (err = false), this.$refs.title.focus());\r\n      err &&\r\n        !this.content &&\r\n        ((msg = \"내용 입력해주세요\"), (err = false), this.$refs.content.focus());\r\n\r\n      if (!err) alert(msg);\r\n      else this.insertBoard();\r\n    },\r\n\r\n    insertBoard() {\r\n      http\r\n        .post(\"/qnaboard\", {\r\n          writer: this.getUserId,\r\n          title: this.title,\r\n          content: this.content,\r\n        })\r\n        .then(({ data }) => {\r\n          if (data == \"success\") {\r\n            alert(\"글등록 처리를 하였습니다.\");\r\n            this.submitted = true;\r\n          } else {\r\n            alert(\"글등록 처리를 하지 못했습니다.\");\r\n          }\r\n        })\r\n        .catch(() => {\r\n          alert(\"서버에 오류가 생겼습니다 ㅈㅅ\");\r\n        });\r\n    },\r\n    moveList() {\r\n      this.$router.push(\"/qnaboard\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#subtitle {\r\n  float: right;\r\n}\r\nbutton {\r\n  margin-left: 30px;\r\n}\r\ntable,\r\ntr,\r\ntd {\r\n  border: 1px solid;\r\n  border-collapse: collapse;\r\n}\r\nth {\r\n  text-align: center;\r\n}\r\ninput,\r\ntextarea {\r\n  text-align: left;\r\n  margin: 5px;\r\n  padding: 5px;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\ntextarea {\r\n  margin-top: 20px;\r\n}\r\ntable {\r\n  width: 50%;\r\n  height: 200px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"]}]}