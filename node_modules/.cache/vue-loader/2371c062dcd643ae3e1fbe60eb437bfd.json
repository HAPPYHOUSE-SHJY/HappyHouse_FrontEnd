orm-control\"\r\n          id=\"writer\"\r\n          ref=\"writer\"\r\n          placeholder=\"작성자를 입력하세요\"\r\n          v-model=\"writer\"\r\n        />\r\n        <label for=\"title\">제목</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"title\"\r\n          ref=\"title\"\r\n          placeholder=\"제목을 입력하세요\"\r\n          v-model=\"title\"\r\n        />\r\n        <label for=\"content\">내용</label>\r\n        <textarea\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"contnet\"\r\n          ref=\"content\"\r\n          placeholder=\"내용을 입력하세요\"\r\n          v-model=\"content\"\r\n        ></textarea>\r\n        <td colspan=\"2\" style=\"height: 50px; text-align: center\">\r\n          <button class=\"btn btn-primary\" @click=\"check\">등록</button>\r\n        </td>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else>\r\n      <h4>성공적으로 글등록이 완료하였습니다!</h4>\r\n      <router-link class=\"btn btn-primary\" to=\"/insertboard\">새 글 작성</router-link>\r\n      |\r\n      <router-link class=\"btn btn-primary\" to=\"/qnaboard\">목록으로</router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport http from \"@/util/http-common\";\r\n//import axios from 'axios';\r\n//const SERVER_URL = process.env.VUE_APP_SERVER_URL;\r\nexport default {\r\n  name: \"InsertBoard\",\r\n  data: function () {\r\n    return {\r\n      info: null,\r\n      loading: true,\r\n      errored: false,\r\n      writer: \"\",\r\n      content: \"\",\r\n      no: \"\",\r\n      regtime: \"\",\r\n      title: \"\",\r\n      submitted: false,\r\n    };\r\n  },\r\n  // mounted() {\r\n  //   http\r\n  //     .get('/insert')\r\n  //     .then((response) => (this.info = response.data))\r\n  //     .catch(() => {\r\n  //       this.errored = true;\r\n  //     })\r\n  //     .finally(() => (this.loading = false));\r\n  // },\r\n\r\n  methods: {\r\n    check() {\r\n      let err = true;\r\n      let msg = \"\";\r\n      !this.writer && ((msg = \"작성자를 입력해주세요\"), (err = false), this.$refs.writer.focus());\r\n      err && !this.title && ((msg = \"제목 입력해주세요\"), (err = false), this.$refs.title.focus());\r\n      err &&\r\n        !this.content &&\r\n        ((msg = \"내용 입력해주세요\"), (err = false), this.$refs.content.focus());\r\n\r\n      if (!err) alert(msg);\r\n      else this.insertBoard();\r\n    },\r\n    insertBoard() {\r\n      http\r\n        .post(\"/qnaboard\", {\r\n          writer: this.writer,\r\n          title: this.title,\r\n          content: this.content,\r\n        })\r\n        .then(({ data }) => {\r\n          if (data == \"success\") {\r\n            alert(\"글등록 처리를 하였습니다.\");\r\n            this.submitted = true;\r\n          } else {\r\n            alert(\"글등록 처리를 하지 못했습니다.\");\r\n          }\r\n        })\r\n        .catch(() => {\r\n          alert(\"서버에 오류가 생겼습니다 ㅈㅅ\");\r\n        });\r\n    },\r\n    moveList() {\r\n      this.$router.push(\"/qnaboard\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#subtitle {\r\n  float: right;\r\n}\r\nbutton {\r\n  margin-left: 30px;\r\n}\r\n</style>\r\n"]}]}